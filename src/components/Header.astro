<style lang="scss">
	.header {
		background-color: var(--color-black);
	}
	.header__inner {
		display: flex;
		justify-content: space-between;
		align-items: center;
		padding: var(--size-1) 0;
	}
	.social-list {
		margin: 0;
		padding: 0;
		display: flex;
	}
	.social-list__item {
		margin: 0;
		padding: 0 calc(var(--adaptive-pixel) * 24);
		list-style-type: none;
	}
	.social-list__link {
		display: block;
		opacity: 0.7;
		&:hover,
		&:focus {
			opacity: 1;
		}
		svg {
			width: calc(var(--adaptive-pixel) * 24);
			height: calc(var(--adaptive-pixel) * 24);
		}
	}
</style>

<style is:global>
	.logo {
		width: calc(var(--adaptive-pixel) * 150);
		height: calc(var(--adaptive-pixel) * 32);
	}
	.logo-icon {
		width: calc(var(--adaptive-pixel) * 62);
		height: calc(var(--adaptive-pixel) * 40);
	}
</style>
