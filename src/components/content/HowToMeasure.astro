<style lang="scss">
    .article {
        background-color: var(--color-bg-black);
        border-left: var(--2px) solid var(--color-product);
        padding: var(--30px) var(--30px) var(--60px);
        margin-bottom: var(--45px);
        font-size: var(--small-font-size);
    }

    .list {
        list-style: none;
        padding-left: 0;
        margin: 0;

        &__item {
            margin: 0 0 var(--30px);
            display: flex;
            flex-flow: row nowrap;
            align-items: center;

            &:nth-child(even) {
                flex-direction: row-reverse;
            }

            &__content {
                margin: 0 var(--15px);
            }

            svg {
                margin: 0 var(--15px);
                width: calc(var(--1px) * 180);
                height: calc(var(--1px) * 180);
            }
        }

        & h5 {
            font-size: inherit;
            display: inline-flex;
            align-items: center;
            margin: 0;
            font-weight: 600;
        }

        & p {
            display: inline;
            margin: 0;
        }
    }

    @media screen and (max-width: 1023px) {
        .article {
            font-size: var(--extra-small-font-size);
            padding: var(--30px) var(--8px) var(--30px);
            margin-bottom: var(--30px);
        }

        .list__item {
            &__content,
            svg {
                margin: 0 var(--8px);
            }

            svg {
                width: var(--90px);
                height: var(--90px);
            }
        }
    }
</style>
